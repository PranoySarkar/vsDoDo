{
	"name": "vsdodo",
	"displayName": "vsDoDo",
	"description": "vsDoDo helps you to plan your Task/Project/Feature and gives you a great visibility on your current progress which helps you to focus and prioritize on your tasks  ",
	"publisher": "PranoySarkar",
	"icon": "assets/logo.png",
	"version": "1.0.6",
	"repository": {
		"url": "https://github.com/prnysarker/vsDoDo.git"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vsdodo.start",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vsdodo.start",
				"title": "start vsDoDo",
				"category": "vsDoDo"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"package":"vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
